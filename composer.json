{
    "name": "ivory/google-map",
    "description": "Google Map API v3 integration for PHP ^8.1",
    "keywords": [ "google", "map"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Breu",
            "email": "samuel.breu@bresam.ch",
            "role": "Developer/Maintainer"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com",
            "role": "Creator/Inactive"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/event-dispatcher": "^6.4 || ^7.0",
        "symfony/serializer": "^6.4 || ^7.0",
        "symfony/property-access": "^6.4 || ^7.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "psr/http-factory": "^1.1",
        "psr/http-client": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.49",
        "php-http/cache-plugin": "^2.0",
        "phpunit/phpunit": "^9.6.16",
        "phpunit/phpunit-selenium": "^9.0",
        "symfony/cache": "^6.4 || ^7.0",
        "symfony/http-client": "^6.4 || ^7.0",
        "symfony/phpunit-bridge": "^6.4 || ^7.0",
        "ext-json": "*",
        "nyholm/psr7": "^1.8"
    },
    "scripts": {
        "test": "vendor/bin/phpunit -c phpunit.ci.xml"
    },
    "suggest": {
        "symfony/http-client": "For a fast PSR-18 client",
        "php-http/guzzle7-adapter": "For a widely used PSR-18 client",
        "nyholm/psr7": "Lightweight PSR-7/17 implementation",
        "php-http/discovery": "To auto-discover installed HTTP clients/factories"
    },
    "replace": {
        "egeloen/google-map": "^2.0.2"
    },
    "autoload": {
        "psr-4": { "Ivory\\GoogleMap\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Ivory\\Tests\\GoogleMap\\": "tests/" }
    }
}
